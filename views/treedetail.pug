extends layout
block content
  h1= title
  if (toShow == null)
    h2 No instance to delete
  else
    - let treeid = toShow._id
    - let tree_species = toShow.tree_species
    - let tree_age = toShow.tree_age
    - let tree_height = toShow.tree_height
  div(class='container')
    div.row
      div(class='treeAttr mr-3  col-sm') Tree ID is:
        div #{treeid}
    div.row
      div(class='treeAttr mr-3 col-sm') Tree Species is:
        div #{tree_species}
    div.row
      div(class='treeAttr mr-3 col-sm') Tree Age is:
        div #{tree_age}
    div.row
      div(class='treeAttr mr-3 col-sm') Tree Height is:
        div #{tree_height}